@model probnik.Data.Photos
@{
    ViewBag.Title = "Удаление Фото";
}

<div>
    <dl class="dl-horizontal">
        <dt>albumid</dt>
        <dd>
            @Html.DisplayFor(model => model.albumId)
        </dd>

        <dt>id</dt>
        <dd>
            @Html.DisplayFor(model => model.Id)
        </dd>

        <dt>Название</dt>
        <dd>
            @Html.DisplayFor(model => model.Title)
        </dd>

        <dt>Фото</dt>
        <dd>
            @Html.DisplayFor(model => model.Photo)
        </dd>

        <div>
            <form asp-controller="Albums" asp-action="DeletePhotos" method="post">
                <div class="form-group">
                    <input type="submit" class="btn btn-default" value="Удалить" />
                </div>
            </form>
        </div>
</div>